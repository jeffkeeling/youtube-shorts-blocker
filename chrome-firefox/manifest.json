{
  "name": "Youtube Shorts Blocker",
  "description": "Block Youtube Shorts from homepage fork of https://github.com/umutseven92/shorts-blocker",
  "version": "1",
  "manifest_version": 3,
  "browser_specific_settings": {
    "gecko_android": {},
    "gecko": {
      "id": "{810c201c-ab85-478a-ae09-da4a3b66f2d9}"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
